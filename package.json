{
    "name": "supersubzero",
    "displayName": "supersub",
    "description": "Convert a string to superscript or subscript if possible.",
    "version": "0.5.0",
    "publisher": "idmytro",
    "repository": "https://github.com/idmytro/supersub",
    "icon": "supersub.png",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:supersub.super",
        "onCommand:supersub.sub"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "supersub.super",
                "title": "Convert to superscript"
            },
            {
                "command": "supersub.sub",
                "title": "Convert to subscript"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^5.3.3",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^20.10.5",
        "@types/mocha": "^2.2.42"
    }
}
